{
  "version": 2,
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ]
        }
    ]
}
